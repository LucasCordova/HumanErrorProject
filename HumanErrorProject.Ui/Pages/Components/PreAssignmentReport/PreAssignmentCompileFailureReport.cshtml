@using HumanErrorProject.Data.Models
@model PreAssignment

<div class="container my-5 bg-light pt-3 pb-5 px-5 shadow-sm">
    <div class="mx-auto text-center">
        <h2 class="display-2 text-danger">
            Compile Error Occured
        </h2>
    </div>
    <p class="lead">
        Output
    </p>
    <div class="bg-dark p-2">
        @foreach (var text in ((PreAssignmentCompileFailureReport)Model.PreAssignmentReport).Report.Split('\n'))
        {
            <code>
                @text
            </code>
            <br/>
        }
    </div>
</div>