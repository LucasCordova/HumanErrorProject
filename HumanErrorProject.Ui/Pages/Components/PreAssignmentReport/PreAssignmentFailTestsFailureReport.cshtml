@using HumanErrorProject.Data.Models
@model PreAssignment

<div class="container my-5 bg-light pt-3 pb-5 px-5 shadow-sm">
    <div class="mx-auto text-center">
        <h2 class="display-2 text-danger">
            Failed Unit Tests!
        </h2>
    </div>
    <table class="table table-striped table-dark">
        <tbody>
        @foreach (var unitTest in ((PreAssignmentFailTestsFailureReport) Model.PreAssignmentReport).FailUnitTests)
        {
            <tr>
                <td>
                    @unitTest.Category
                </td>
                <td>
                    @unitTest.Name
                </td>
            </tr>
        }
        </tbody>
    </table>
</div>